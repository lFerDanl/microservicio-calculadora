generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Operacion {
  id         String   @id @default(uuid())
  tipo       String
  a          Float
  b          Float
  resultado  Float
  createdAt  DateTime @default(now()) @map("created_at")

  @@map("operaciones")
}


